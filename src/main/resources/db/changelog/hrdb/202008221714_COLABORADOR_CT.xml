<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

    <changeSet author="luifer" id="hr_colaborador_ct_001">
        <createTable tableName="colaborador" schemaName="management">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="nombre" type="VARCHAR">
                <constraints nullable="false"/>
            </column>
            <column name="apellido" type="VARCHAR">
                <constraints nullable="false"/>
            </column>
            <column name="curriculum_url" type="VARCHAR">
                <constraints nullable="true"/>
            </column>
            <column name="nro_celular" type="VARCHAR">
                <constraints nullable="true"/>
            </column>
            <column name="email" type="VARCHAR">
                <constraints nullable="false"/>
            </column>
            <column name="comentario" type="VARCHAR">
                <constraints nullable="true"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>